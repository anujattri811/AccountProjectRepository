<script>
    $('#RequestedDocumentForm').validate({
        rules: {
            DocumentType: {
                required: true,
            },
            Year: {
                required: true,
            },
            PersonName: {
                required: true,
            },
            Description: {
                required: true,
            },
        },
        messages: {
            DocumentType: {
                required: "Please select Document Type.",
            },
            Year: {
                required: "Please select year.",
            },
            PersonName: {
                required: "Please select person name.",
            },
            Description: {
                required: "Please select description.",
            },
        },
        errorElement: 'span',
        errorPlacement: function (error, element) {
            error.addClass('invalid-feedback');
            element.closest('.form-group').append(error);
        },
        highlight: function (element, errorClass, validClass) {
            $(element).addClass('is-invalid');
        },
        unhighlight: function (element, errorClass, validClass) {
            $(element).removeClass('is-invalid');
        }
    });

    function ShowOther() {
        if ($("#DocumentType").val() == "Other") {
            $("#Show-OtherDocuments").fadeIn();
            $("#Show-years").fadeOut();
            $("#Show-years").fadeIn();
            $("#Quaterly").fadeOut();


        }
        else if ($("#DocumentType").val() == "GST") {
            $("#Quaterly").fadeOut();
            $("#Show-years").fadeOut();
            $("#Show-years").fadeIn();
            $("#Show-OtherDocuments").fadeOut();
        }
        else {
            if ($("#DocumentType").val() == "Paystubs" || $("#DocumentType").val() == "PD7 A") {
                $("#Show-years").fadeOut();
                $("#Show-years").fadeIn();
                $("#Show-OtherDocuments").fadeOut();
                $("#Quaterly").fadeOut();
            }
            else {
                $("#Show-OtherDocuments").fadeOut();
                $("#Show-years").fadeOut();
                $("#Year").fadeIn();
                $("#Quaterly").fadeOut();
                $("#Show-months").fadeOut();
                $("#PeriodTime").val($("#PeriodTime option:first").val());
                $("#RequestMonthly").val($("#RequestMonthly option:first").val());
                $("#RequestQuaterly").val($("#RequestQuaterly option:first").val());
                $("#RequestYear").val($("#RequestYear option:first").val());
            }

        }
    }

    function Test() {
        if ($("#PeriodTime").val() == "Monthly") {
            $("#Show-months").fadeIn();
            $("#Quaterly").fadeOut();
        }
        else if ($("#PeriodTime").val() == "Quaterly" && $("#DocumentType").val() == "GST") {
            $("#Quaterly").fadeIn();
            $("#Show-OtherDocuments").fadeOut();
            $("#Show-months").fadeOut();
        }
        else {
            $("#Show-months").fadeOut();
            $("#Quaterly").fadeOut();
        }
    }

    function ShowOtherPerson() {
        if ($("#PersonName").val() == "Other") {
            $("#Show-OtherPersonName").fadeIn();
        }
        else {
            $("#Show-OtherPersonName").fadeOut();
        }
    }

    function SearchByMainField() {
        if ($("#SearchDocumentType").val() == "GST" || $("#SearchDocumentType").val() == "Paystubs" || $("#SearchDocumentType").val() == "PD7 A") {
            $("#SearchQuaterly").fadeOut();
            $("#SearchMonthly").fadeOut();
            $("#SearchPeriodEnding").fadeIn();

        }
        else {
            $("#SearchPeriodEnding").fadeOut();
            $("#SearchMonthly").fadeOut();
            $("#SearchQuaterly").fadeOut();
            $("#SearchPeriodEndingTime").val($("#SearchPeriodEndingTime option:first").val());
            $("#SearchSelectQuaterTime").val($("#SearchSelectQuaterTime option:first").val());
            $("#SearchSelectMonthTime").val($("#SearchSelectMonthTime option:first").val());
            $("#SearchSelectYearTime").val($("#SearchSelectYearTime option:first").val());
        }
    }

    function SearchByPeriodEnding() {
        if ($("#SearchPeriodEndingTime").val() == "Monthly") {
            $("#SearchMonthly").fadeIn();
            $("#SearchQuaterly").fadeOut();
        }
        else if ($("#SearchPeriodEndingTime").val() == "Quaterly" && $("#SearchDocumentType").val() == "GST") {
            $("#SearchQuaterly").fadeIn();
            $("#SearchMonthly").fadeOut();
        }
        else {
            $("#SearchMonthly").fadeOut();
            $("#SearchQuaterly").fadeOut();
        }
    }


    function Delete(UserId) {
        Swal.fire({
            title: 'Are you sure?',
            text: "You won't be able to revert this!",
            icon: 'warning',
            showCancelButton: true,
            confirmButtonColor: '#3085d6',
            cancelButtonColor: '#d33',
            confirmButtonText: 'Yes, delete it!'
        }).then((result) => {
            if (result.isConfirmed) {
                window.location.href = "/Client/DeleteRequest?UserId=" + UserId

            }
            else {
                Swal.fire(
                    'Cancelled',
                    'Your document is safe :)',
                    'error'
                )
            }
        })
    }
</script>
