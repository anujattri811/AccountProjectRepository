                    </div>
                    <div class="">
                        <label>Password:</label>
                        <label style="font-weight:400;">@Model.Password</label>
                    </div>
                </div>
                <div class="modal-footer justify-content-between">
                    <button type="submit" class="btn btn-block btn-outline-primary col-md-4">Send It To Client</button>
                    <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>

                </div>
            }

        </div>
        <!-- /.modal-content -->
    </div>
    <!-- /.modal-dialog -->
