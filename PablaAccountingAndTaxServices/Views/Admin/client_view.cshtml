<script>
    function GeneratePassword(UserId, Email, FirstName, MobileNo, DateOfBirth) {
        $.ajax({
            type: "POST",
            url: '/Admin/GeneratePassword',
            data: {
                UserId: UserId,
                Email: Email,
                FirstName: FirstName,
                MobileNo: MobileNo,
                DateOfBirth: DateOfBirth
            },
            success: function () {

            }

        });
        return false;
    }



</script>

<script type="text/javascript">

    $('#Savedocument').validate({
        rules: {
            PersonName: {
                required: true,
            },
            UploadFile: {
                required: true,
            },
            OtherDocuments: {
                required: true,
            }

        },
        messages: {
            PersonName: {
                required: "Please enter a Person  Name. ",
            },
            UploadFile: {
                required: "Please Select a File.",
            },
            OtherDocuments: {
                required: "Please enter other document type.",
            }



        },
        errorElement: 'span',
        errorPlacement: function (error, element) {
            error.addClass('invalid-feedback');
            element.closest('.form-group').append(error);
        },
        highlight: function (element, errorClass, validClass) {
            $(element).addClass('is-invalid');
        },
        unhighlight: function (element, errorClass, validClass) {
            $(element).removeClass('is-invalid');
        }
    });
</script>
<script>
    function ShowOther() {
        if ($("#DocumentType").val() == "Other") {
            $("#Show-OtherDocuments").fadeIn();
            $("#PeriodEnding").fadeIn();
            $("#SelectMonth").fadeOut();
            $("#SelectQuaterly").fadeOut();
            //$("#selectPeriodMonthly").val($("#selectPeriodMonthly option:first").val());
            //$("#selectPeriodQuaterly").val($("#selectPeriodQuaterly option:first").val());
            $("#selectPeriodQuaterly").val("");
            $("#selectPeriodMonthly").val("");
            $("#year").val("");
        }
        else if ($("#DocumentType").val() == "GST") {
            $("#Show-OtherDocuments").fadeOut();
            $("#PeriodEnding").fadeIn();
            $("#SelectMonth").fadeOut();
            $("#SelectQuaterly").fadeOut();
             $("#selectPeriodQuaterly").val("");
            $("#selectPeriodMonthly").val("");
            $("#year").val("");
            //$("#selectPeriodMonthly").val($("#selectPeriodMonthly option:first").val());
            //$("#selectPeriodQuaterly").val($("#selectPeriodQuaterly option:first").val());
        }
        else if ($("#DocumentType").val() == "Paystubs" || $("#DocumentType").val() == "PD7 A") {
            $("#Show-OtherDocuments").fadeOut();
            $("#PeriodEnding").fadeOut();
            $("#SelectMonth").fadeIn();
            $("#SelectQuaterly").fadeOut();
            //$("#selectPeriodMonthly").val($("#selectPeriodMonthly option:first").val());
            //$("#selectPeriodQuaterly").val($("#selectPeriodQuaterly option:first").val());
            $("#selectPeriodQuaterly").val("");
            $("#selectPeriodMonthly").val("");
            $("#year").val("");
        }
        else {
            $("#Show-OtherDocuments").fadeOut();
            $("#PeriodEnding").fadeOut();
            $("#SelectMonth").fadeOut();
            $("#SelectQuaterly").fadeOut();
            $("#selectPeriodQuaterly").val("");
            $("#selectPeriodMonthly").val("");
            $("#year").val("");
            $("#selectPeriodEnding").val("");
            //$("#selectPeriodEnding").val($("#selectPeriodEnding option:first").val());
            //$("#selectPeriodMonthly").val($("#selectPeriodMonthly option:first").val());
            //$("#selectPeriodQuaterly").val($("#selectPeriodQuaterly option:first").val());
        }
    }


    function OpenByPeriod() {
        if ($("#selectPeriodEnding").val() == "Monthly") {
            $("#SelectMonth").fadeIn();
            $("#SelectQuaterly").fadeOut();
            $("#selectPeriodQuaterly").val($("#selectPeriodQuaterly option:first").val());
        }
        else if ($("#selectPeriodEnding").val() == "Annualy") {
            $("#SelectMonth").fadeOut();
            $("#SelectQuaterly").fadeOut();
            $("#selectPeriodMonthly").val($("#selectPeriodMonthly option:first").val());
            $("#selectPeriodQuaterly").val($("#selectPeriodQuaterly option:first").val());
        }
        else if ($("#selectPeriodEnding").val() == "Quarterly" && $("#DocumentType").val() == "GST" || $("#DocumentType").val() == "Other") {
            $("#SelectQuaterly").fadeIn();
            $("#SelectMonth").fadeOut();
            $("#selectPeriodMonthly").val($("#selectPeriodMonthly option:first").val());
        }

    }


    function SearchByOtherField() {
        if ($("#DocumentTypes").val() == "GST") {
            $("#SearchPeriodEnding").fadeIn();
            $("#SearchPeriodMonthly").fadeOut();
            $("#SearchPeriodQuaterly").fadeOut();
            $("#SelectSearchMonth").val($("#SelectSearchMonth option:first").val());
            $("#SelectSearchQuaterly").val($("#SelectSearchQuaterly option:first").val());
        }
        else if ($("#DocumentTypes").val() == "Paystubs" || $("#DocumentTypes").val() == "PD7 A") {
            $("#SearchPeriodEnding").fadeOut();
            $("#SearchPeriodMonthly").fadeIn();
            $("#SearchPeriodQuaterly").fadeOut();
            $("#SelectSearchQuaterly").val($("#SelectSearchQuaterly option:first").val());
        }
        else {
            $("#SearchPeriodEnding").fadeOut();
            $("#SearchPeriodMonthly").fadeOut();
            $("#SearchPeriodQuaterly").fadeOut();
            $("#SelectSearchPeriodEnding").val($("#SelectSearchPeriodEnding option:first").val());
            $("#SelectSearchMonth").val($("#SelectSearchMonth option:first").val());
            $("#SelectSearchQuaterly").val($("#SelectSearchQuaterly option:first").val());
        }
    }
    function OpenBySearchPeriod() {
        if ($("#SelectSearchPeriodEnding").val() == "Monthly") {
            $("#SearchPeriodMonthly").fadeIn();
            $("#SearchPeriodQuaterly").fadeOut();
            $("#SelectSearchQuaterly").val($("#SelectSearchQuaterly option:first").val());
        }
        else if ($("#SelectSearchPeriodEnding").val() == "Quarterly" && $("#DocumentTypes").val() == "GST") {
            $("#SearchPeriodQuaterly").fadeIn();
            $("#SearchPeriodMonthly").fadeOut();
            $("#SelectSearchMonth").val($("#SelectSearchMonth option:first").val());
        }
        else {
            $("#SearchPeriodQuaterly").fadeOut();
            $("#SearchPeriodMonthly").fadeOut();
            $("#SelectSearchPeriodEnding").val($("#SelectSearchPeriodEnding option:first").val());
            $("#SelectSearchMonth").val($("#SelectSearchMonth option:first").val());
            $("#SelectSearchQuaterly").val($("#SelectSearchQuaterly option:first").val());
        }
    }
</script>


