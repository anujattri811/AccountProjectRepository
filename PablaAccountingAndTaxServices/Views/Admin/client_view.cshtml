<section class="content header">
    <div class="card">
        <div class="card-header card_color" style="padding-top: 1px;padding-bottom: 1px;">
            <div class="col-md-12">
                <div class="col-md-6">
                    <h3 class="card-title" style="color: white;padding-top:10px;padding-bottom: 10px;">Document Information</h3>

                </div>

            </div>
        </div>
        <div class="card-body" style="padding:0px;">
            <div id="example1_wrapper" class="dataTables_wrapper dt-bootstrap4" style="padding-top:10px;">
                <div class="col-sm-12" style="overflow-x:auto;">
                    <table id="example1" class="table table-bordered table-striped dataTable" role="grid" aria-describedby="example1_info">
                        <thead>
                            <tr role="row" class="table-color">
                                <th>Operation</th>
                                <th>Person Name</th>
                                <th>Document Name</th>
                                <th>Document Type</th>
                                <th>Other</th>
                                @*<th>Month</th>*@
                                <th>Year</th>
                                <th>Monthly</th>
                                <th>Quarterly</th>
                            </tr>
                        </thead>

                        <tbody>
                            @foreach (var item in ViewBag.TotalDocument)
                            {
                                <tr>
                                    <td>
                                        <a href="~/Documents/@item.DocumentName.@item.Discription"><span class="info-box-icon" style="font-size:20px;"><i class="fa fa-eye"></i></span></a>
                                        <a href="/Admin/DownloadFile?FileName=@item.DocumentName.@item.Discription" target="_blank"><span class="info-box-icon" style="font-size:20px;"><i class="fas fa-cloud-download-alt"></i></span></a>
                                        <a class="btn btn-danger btn-sm" href="#" onclick="Delete(@item.DocumentId);" style="padding: 0px 7px;margin-top: -6px;">
                                            <i class="fas fa-trash" style="font-size:12px;">
                                            </i>

                                        </a>
                                    </td>
                                    <td>@item.PersonName</td>
                                    <td>@item.DocumentName</td>
                                    <td>@item.DocumentType</td>
                                    <td>@item.Other</td>
                                    @*<td>@item.Month</td>*@
                                    <td>@item.Year</td>
                                    <td>@item.Monthly</td>
                                    <td>@item.Quaterly</td>
                                </tr>
                            }

                        </tbody>

                    </table>
                </div>
            </div>
        </div>
    </div>


</section>
